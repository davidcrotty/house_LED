upstream django {
    server 127.0.0.1:8001;
}

server {
    listen 80 default_server;
    listen [::]:80 default_server ipv6only=on;

    root /var/www/sites/house-led.dev;
    index index.html index.htm;

    server_name house-led.dev;
    charset utf-8;

    client_max_body_size 75M;

    location /favicon.ico {
	     access_log off;
	     log_not_found off;
    }

    location /media {
	     root /var/www/sites/house-led.dev/house_led/media;
    }

   location /static {
	   root /var/www/sites/house-led.dev/house_led/static;
   }

   location / {
    	# First attempt to serve request as file, then
    	# as directory, then fall back to displaying a 404.
    	#try_files $uri $uri/ =404;
    	uwsgi_pass unix:/var/www/sites/house-led.dev/house_led/django.sock;
    	include /var/www/sites/house-led.dev/house_led/uwsgi_params;
   }
}
